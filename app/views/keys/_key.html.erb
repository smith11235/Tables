<h3><%= key.name %></h3>
<div id="<%= key.name %>" style="height:auto;min-height:100px" >
	<%= link_to "Show Records", data_set_key_url(key.data_set,key), :remote => true %><br />
	<script>
		$('a').bind('ajax:success', function(event, xhr, status, data) {  
			var table = JSON.parse(data.responseText);
			setMainDisplay( table );
		} );
	</script>
	<%= link_to 'Re-index This Key', find_records_data_set_key_url( key.data_set, key ) %>
	<h3>Stats:</h3>
	Fields: <%= key.unique_field_names.join( "," ) %><br />
	Unique Records: <%= key.records.size %><br />
	Unique Keys: <br />
	Duplicate key distribution <br />
</div>
