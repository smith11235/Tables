<h1>Listing key_fields</h1>

<table>
  <tr>
    <th>Key</th>
    <th>Field</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @key_fields.each do |key_field| %>
  <tr>
    <td><%= key_field.key %></td>
    <td><%= key_field.field %></td>
    <td><%= link_to 'Show', key_field %></td>
    <td><%= link_to 'Edit', edit_key_field_path(key_field) %></td>
    <td><%= link_to 'Destroy', key_field, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Key field', new_key_field_path %>
